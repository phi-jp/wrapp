<?xml version='1.0' encoding='utf-8'?>
<widget id="jp.phi.FlickArrow" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="AdMob">
        <param name="ios-package" value="CDVAdMobPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SocialMessage">
        <param name="ios-package" value="SocialMessage" />
    </feature>
    <feature name="GameCenter">
        <param name="ios-package" value="GameCenter" />
    </feature>
    <icon height="60" src="res/ios/Icon-60.png" width="60" />
    <icon height="120" src="res/ios/icon-72@2x.png" width="120" />
    <icon height="76" src="res/ios/Icon-72.png" width="76" />
    <icon height="152" src="res/ios/Icon-72@2x.png" width="152" />
    <icon height="40" src="res/ios/icon-60.png" width="40" />
    <icon height="80" src="res/ios/icon-72@2x.png" width="80" />
    <icon height="57" src="res/ios/Icon.png" width="57" />
    <icon height="114" src="res/ios/Icon@2x.png" width="114" />
    <icon height="72" src="res/ios/Icon-72.png" width="72" />
    <icon height="144" src="res/ios/Icon-72@2x.png" width="144" />
    <icon height="180" src="res/ios/Icon-72@2x.png" width="180" />
    <icon height="50" src="res/ios/Icon-Small-50.png" width="50" />
    <icon height="100" src="res/ios/Icon-Small-50@2x.png" width="100" />
    <icon height="29" src="res/ios/Icon-60.png" width="29" />
    <icon height="58" src="res/ios/Icon-60.png" width="58" />
    <name>FlickArrow</name>
    <description>
        A simple game.
    </description>
    <author email="phi1618jp@gmail.com" href="http://tmlife.net">
        tmlib.js project
    </author>
    <content src="flick-arrow.html" />
    <access origin="*" />
</widget>
